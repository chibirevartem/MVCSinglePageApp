@model IEnumerable<WebService.Models.Entities.Organisation>

<div class="panel-heading">All the organisations</div>
<div class="panel-body">
    <table class="table table-striped table-condensed">
        <thead>
            <tr>
                <th>Name</th>
                <th>Tax Id</th>
                <th>Registration reason code</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var organisation in Model)
            {
            <tr>
                <td>@organisation.Name</td>
                <td>@organisation.TaxId</td>
                <td>@organisation.RegistrationReasonCode</td>
                <td>
                    @Html.ActionLink("Remove","Remove",
                   new { id = organisation.TaxId},
                   new { @class = "btn btn-xs btn-primary"})
                </td>
            </tr>
            }
        </tbody>
    </table> 
</div>

